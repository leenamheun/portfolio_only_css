// Any keys added to this array will be auto-detected from the query string,
// and set in the PARSED_FLAGS object as well as in a cookie
var ACTIVE_FLAGS = ['cjevent', 'coupon', 'ref', 'gclid']

// the trailing '.' marks the cookie as accessible in all logo.com subdomains.
// Change this to whatever domain you need for testing
var LOGO_DOMAIN = '.logo.com'

/**
* Converts the windows query string into an object literal, s
* where the key becomes the property name and the value 
* becomes the property value.  Only keys found inside 
* ACTIVE_FLAGS will be parsed.  An empty or non-existent
* query string will result in an empty object literal.
* Empty query string values, like cjevent= will be skipped
*
* Example:
*
* ?coupon=100off&not_parsed=true&cjevent=
* becomes:
* {
*   coupon: '100off'
* }
*/
function parseActiveFlags() {
    // A query string will either be empty, or start with '?', so we strip that off.
    var query = window.location.search.substring(1)
    var parsed_object = {}
    
    if (!query) {
        return parsed_object
    }

    var key_value_statements = query.split('&')

    for (let i = 0; i < key_value_statements.length; i++) {
        var pair = key_value_statements[i].split('=')
        var key = decodeURIComponent(pair[0])
        var value = decodeURIComponent(pair[1])

        // ?coupon= is the same as not being set at all.  ignore it
        if (value === '') {
            continue
        } else if (ACTIVE_FLAGS.indexOf(key) >= 0) {
            parsed_object[key] = value
        }
    }

    return parsed_object
}


function setCookie(name, value, expires, domain) {
    document.cookie = name + "=" + value + ";expires=" + expires + ";path=/;domain=" + domain;
}

/**
* Takes in an object literal, and sets each key as a cookie, expiring
* 1 year after being set.  The cookies domain is set to LOGO_DOMAIN, so
* ensure that you've updated that if you're testing on a different domain.
*/
function createCookies(flags) {
    // 1 year expiry, which needs to be converted to UTC format
    var date = new Date()
    date.setTime(date.getTime() + (30 * 24 * 60 * 60 * 1000))
    var expires = date.toUTCString()


    var keys = Object.keys(flags)
    for (let i = 0; i < keys.length; i++) {
        setCookie(keys[i], flags[keys[i]], expires, LOGO_DOMAIN)
    }
}

var PARSED_FLAGS = parseActiveFlags()
createCookies(PARSED_FLAGS) 

